<link href='https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.8.2/fullcalendar.min.css' rel='stylesheet' />
<link href='https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.8.2/fullcalendar.print.css' rel='stylesheet' media='print' />
<style type="text/css">
	.popover-content {
	    border-radius: 0 0 4px 4px;
	}
	.popover-title{
		color: #176de5;
    	font-weight: 600;
		background-color: #f1f1f1;
		border-bottom: 1px solid #ccc;
		border-radius: 4px 4px 0 0;
	}
	.popover {
	    border: none;
	}

</style>
 <div class="calendar-main">
	 <div class="row-fluid">
		<div class="span12">
		  <!-- Nav tabs -->
		  <ul class="nav nav-tabs" role="tablist" id = "calendar-tabs">
		  </ul>
		</div>
	 </div>
	<div class="row-fluid">
		<div class="span12" id='calendar'></div>
		<div id="confirmation_view" class = "hide">
			<div class="tabs-heading">
				{{str 'LBL_PLACEMENT_CONFIRMATION' this.module}}
			</div>
			<div id ="confirmation_list_view">
			</div>
			<div class ="row-fluid" id = "confirmation_view_footer">
		    {{#each meta.buttons}}
		        {{field ../this model=../model}}
		    {{/each}}
			</div>
		</div>
	</div>
</div>